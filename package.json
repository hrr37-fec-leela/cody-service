{
  "name": "under-armour",
  "version": "1.0.0",
  "description": "shoe description and shoe carousel",
  "author": "Cody Smith",
  "license": "",
  "engines": {
    "node": ">=6.13.0"
  },
  "scripts": {
    "build": "webpack -d --watch",
    "start": "nodemon server/index.js -w database -w server",
    "seed-shoeDisplay": "mongoimport --db underarmour --collection shoe_display --file shoeDisplay.json --jsonArray",
    "seed-shoeInfo": "mongoimport --db underarmour --collection shoe_info --file shoeInfo.json --jsonArray",
    "seed-shoeInventory": "mongoimport --db underarmour --collection shoe_inventory --file shoeInventory.json --jsonArray",
    "test": "jest --verbose",
    "test-coverage": "jest --verbose --coverage",
    "update-snaps": "jest --updateSnapshot",
    "eslint": "eslint --ext .jsx,.js client server database"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "faker": "^4.1.0",
    "mongodb": "^3.2.2",
    "mongoose": "^5.4.20",
    "nodemon": "^1.18.10",
    "path": "^0.12.7",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "^2.1.8"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.7.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint-config-hackreactor": "git://github.com/reactorcore/eslint-config-hackreactor.git",
    "jest": "^24.7.0",
    "ng-faker": "^1.1.0",
    "react-test-renderer": "^16.8.6",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev": "^1.1.1"
  }
}
